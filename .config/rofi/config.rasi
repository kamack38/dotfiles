// Source: https://github.com/cxOrz/dotfiles-hyprland/blob/main/.config/rofi/config.rasi
configuration {
  font: "Source Code Pro 16px";
  location: 0;
  fixed-num-lines: true;
  show-icons: true;
  scroll-method: 1;
  cycle: true;
  matching: "fuzzy";
  kb-accept-entry: "Return";
  kb-row-up: "Control+k";
  kb-row-down: "Control+j";
  kb-remove-to-eol: "Control+End";
}

@import "theme"
