def_key "k"
 scroll_up

def_key "j"
 scroll_down

def_key "alt-k"
  move_selected_items_up

def_key "alt-j"
  move_selected_items_down

def_key "ctrl-u"
  page_up

def_key "ctrl-d"
  page_down

def_key "g"
  move_home

def_key "G"
  move_end

def_key "d"
  delete_playlist_items

def_key "d"
  delete_browser_items

def_key "space"
  select_item
