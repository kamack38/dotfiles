{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "transient_prompt": {
        "template": "<#3BD01D>‚ùØ</> ",
        "foreground": "#ffffff",
        "background": "transparent"
    },
    "console_title_template": "{{if .Root}}‚ö° {{end}}{{ .Shell }} in {{ .Folder }}",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "properties": {
                        "alpine": "<#0D597F>\uf300</>",
                        "arch": "<#1793d1>\uf303</>",
                        "centos": "\uf304",
                        "debian": "<#A90232>\uf306</>",
                        "elementary": "<#44A2E8>\uf309</>",
                        "fedora": "<#072B61>\uf30a</>",
                        "gentoo": "<#61538D>\uf30d</>",
                        "linux": "<#F5C11B>\ue712</>",
                        "macos": "\ue711",
                        "manjaro": "<#34BE5B>\uf312</>",
                        "mint": "<#86BE43>\uf30f</>",
                        "opensuse": "<73BA25>\uf314</>",
                        "raspbian": "<#C51A4A>\uf315</>",
                        "ubuntu": "<#E95420>\uf31c</>",
                        "windows": "<#26C6DA>\ue70f</>"
                    },
                    "style": "diamond",
                    "leading_diamond": "\ue0b6",
                    "template": " {{ if .WSL }}<#26C6DA>\ue70f</>/{{ end }}{{.Icon}} ",
                    "foreground": "#d2ff5e",
                    "background": "#18354F",
                    "type": "os"
                },
                {
                    "properties": {
                        "home_icon": "Û∞ãú",
                        "mapped_locations": {
                            "~/.config": "\ue615",
                            "~/Desktop": "Û∞áÑ",
                            "~/Documents": "Û∞àô",
                            "~/Documents/CPlusPlus": "\ue61d",
                            "~/Documents/GitHUB": "\uf408",
                            "~/Documents/cpp": "\ue61d",
                            "~/Downloads": "\uf019",
                            "~/Music": "\uf001",
                            "~/Pictures": "Û∞ã´",
                            "~/Videos": "\uf03d"
                        },
                        "style": "full"
                    },
                    "style": "powerline",
                    "foreground": "#38DE21",
                    "powerline_symbol": "\ue0b0",
                    "background": "#2e6f95",
                    "type": "path"
                },
                {
                    "properties": {
                        "branch_icon": "\uf126 ",
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "powerline",
                    "foreground": "#f7fff7",
                    "powerline_symbol": "\ue0b0",
                    "background": "#4ecdc4",
                    "type": "git",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#EDC809{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#F26D50{{ end }}",
                        "{{ if gt .Ahead 0 }}#B56576{{ end }}",
                        "{{ if gt .Behind 0 }}#6D597A{{ end }}"
                    ]
                },
                {
                    "properties": {
                        "fetch_status": true
                    },
                    "style": "powerline",
                    "foreground": "#193549",
                    "powerline_symbol": "\ue0b0",
                    "background": "#ffeb3b",
                    "type": "svn"
                }
            ],
            "newline": true
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "style": "powerline",
                    "foreground": "#111111",
                    "powerline_symbol": "\ue0b2",
                    "background": "#ffff66",
                    "type": "root",
                    "invert_powerline": true
                },
                {
                    "properties": {
                        "fetch_package_manager": true
                    },
                    "style": "powerline",
                    "template": "{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{else}} \uf898 {{ end }}{{ .Full }}",
                    "foreground": "#ffffff",
                    "powerline_symbol": "\ue0b2",
                    "background": "#689f63",
                    "type": "node",
                    "invert_powerline": true
                },
                {
                    "style": "powerline",
                    "template": " üåä {{ .Full }} ",
                    "foreground": "#ffffff",
                    "powerline_symbol": "\ue0b2",
                    "background": "#002e53",
                    "type": "nx",
                    "invert_powerline": true
                },
                {
                    "style": "powerline",
                    "foreground": "#03589c",
                    "powerline_symbol": "\ue0b2",
                    "background": "#2bb1ee",
                    "type": "dart",
                    "invert_powerline": true
                },
                {
                    "style": "powerline",
                    "foreground": "#ffffff",
                    "powerline_symbol": "\ue0b2",
                    "background": "#4063D8",
                    "type": "php",
                    "invert_powerline": true
                },
                {
                    "style": "powerline",
                    "template": " R {{ .Full }} ",
                    "foreground": "blue",
                    "powerline_symbol": "\ue0b2",
                    "background": "lightWhite",
                    "type": "r",
                    "invert_powerline": true
                },
                {
                    "style": "powerline",
                    "foreground": "#ffffff",
                    "powerline_symbol": "\ue0b2",
                    "background": "#c3002f",
                    "type": "angular"
                },
                {
                    "properties": {
                        "fetch_version": true
                    },
                    "style": "powerline",
                    "foreground": "#111111",
                    "powerline_symbol": "\ue0b2",
                    "background": "#00acd7",
                    "type": "go",
                    "invert_powerline": true
                },
                {
                    "properties": {
                        "fetch_version": true
                    },
                    "style": "powerline",
                    "foreground": "#111111",
                    "powerline_symbol": "\ue0b2",
                    "background": "#4063D8",
                    "type": "julia",
                    "invert_powerline": true
                },
                {
                    "style": "powerline",
                    "foreground": "#000000",
                    "powerline_symbol": "\ue0b0",
                    "background": "#ebcc34",
                    "type": "kubectl"
                },
                {
                    "properties": {
                        "display_mode": "files",
                        "fetch_version": true,
                        "fetch_virtual_env": false
                    },
                    "style": "powerline",
                    "foreground": "#4584B6",
                    "powerline_symbol": "\ue0b2",
                    "background": "#FFDE57",
                    "type": "python",
                    "invert_powerline": true
                },
                {
                    "style": "powerline",
                    "foreground": "#ffffff",
                    "powerline_symbol": "\ue0b2",
                    "background": "#906cff",
                    "type": "kotlin",
                    "invert_powerline": true
                },
                {
                    "style": "powerline",
                    "foreground": "#ffffff",
                    "powerline_symbol": "\ue0b2",
                    "background": "#4063D8",
                    "type": "crystal",
                    "invert_powerline": true
                },
                {
                    "properties": {
                        "display_mode": "files",
                        "fetch_version": true
                    },
                    "style": "powerline",
                    "foreground": "#ffffff",
                    "powerline_symbol": "\ue0b2",
                    "background": "#AE1401",
                    "type": "ruby",
                    "invert_powerline": true
                },
                {
                    "properties": {
                        "fetch_version": true
                    },
                    "style": "powerline",
                    "foreground": "#E64747",
                    "powerline_symbol": "\ue0b2",
                    "background": "#29315A",
                    "type": "java",
                    "invert_powerline": true
                },
                {
                    "properties": {
                        "fetch_version": true
                    },
                    "style": "powerline",
                    "foreground": "#ffffff",
                    "powerline_symbol": "\ue0b2",
                    "background": "#4E2BCD",
                    "type": "dotnet",
                    "invert_powerline": true
                },
                {
                    "style": "powerline",
                    "template": " \ue7a8 {{ .Full }} ",
                    "foreground": "#193549",
                    "powerline_symbol": "\ue0b2",
                    "background": "#99908a",
                    "type": "rust",
                    "invert_powerline": true
                },
                {
                    "style": "powerline",
                    "template": " \ue620 {{ .Full }} ",
                    "foreground": "#fff",
                    "powerline_symbol": "\ue0b2",
                    "background": "#000080",
                    "type": "lua",
                    "invert_powerline": true
                },
                {
                    "style": "powerline",
                    "template": " \ue61d cmake {{ .Full }} ",
                    "foreground": "#E8EAEE",
                    "powerline_symbol": "\ue0b2",
                    "background": "#1E9748",
                    "type": "cmake",
                    "invert_powerline": true
                },
                {
                    "properties": {
                        "display_mode": "files",
                        "fetch_version": false
                    },
                    "style": "powerline",
                    "foreground": "#ffffff",
                    "powerline_symbol": "\ue0b2",
                    "background": "#FEAC19",
                    "type": "azfunc",
                    "invert_powerline": true
                },
                {
                    "properties": {
                        "display_default": false
                    },
                    "style": "powerline",
                    "foreground": "#ffffff",
                    "powerline_symbol": "\ue0b2",
                    "background": "",
                    "type": "aws",
                    "background_templates": [
                        "{{if contains \"default\" .Profile}}#FFA400{{end}}",
                        "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
                    ],
                    "invert_powerline": true
                },
                {
                    "style": "powerline",
                    "foreground": "#fefefe",
                    "powerline_symbol": "\ue0b2",
                    "background": "#5c41e2",
                    "type": "terraform",
                    "invert_powerline": true
                },
                {
                    "style": "powerline",
                    "foreground": "#FFFFFF",
                    "powerline_symbol": "\ue0b2",
                    "background": "#0077C2",
                    "type": "shell",
                    "invert_powerline": true
                },
                {
                    "properties": {
                        "style": "austin"
                    },
                    "style": "powerline",
                    "foreground": "#293241",
                    "powerline_symbol": "\ue0b2",
                    "background": "#ffe66d",
                    "type": "executiontime",
                    "invert_powerline": true
                },
                {
                    "properties": {
                        "always_enabled": true
                    },
                    "style": "diamond",
                    "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf42e{{ end }} ",
                    "foreground": "#d3d7cf",
                    "powerline_symbol": "\ue0b0",
                    "background": "#3BD01D",
                    "type": "exit",
                    "background_templates": [
                        "{{ if gt .Code 0 }}#FF005D{{ end }}"
                    ],
                    "invert_powerline": true
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "style": "plain",
                    "template": "‚ùØ",
                    "foreground": "#3BD01D",
                    "background": "",
                    "type": "text"
                }
            ],
            "newline": true
        },
        {
            "type": "rprompt",
            "alignment": "right",
            "segments": [
                {
                    "properties": {
                        "track_separator": " - "
                    },
                    "style": "diamond",
                    "leading_diamond": "\ue0b6",
                    "trailing_diamond": "\ue0b4",
                    "foreground": "#ffffff",
                    "background": "#1BD760",
                    "type": "spotify"
                }
            ]
        }
    ],
    "version": 3,
    "final_space": true
}
