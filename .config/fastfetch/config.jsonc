{
    "logo": {
        "source": "~/.config/fastfetch/38.txt",
        "type": "file",
        "padding": { "right": 3 },
        "color": {
            "1": "red"
        }
    },
    "display": {
        "separator": "  ",
        "color": { "separator": "cyan" }
    },
    "modules": [
        { "type": "title", "color": { "user": "blue", "host": "blue" } },
        {
            "type": "custom",
            "format": "────────────────────────────────",
            "outputColor": "cyan"
        },
        { "type": "os", "key": " ", "keyColor": "yellow" },
        {
            "type": "host",
            "format": "{5} {2}",
            "key": "󰌢 ",
            "keyColor": "magenta"
        },
        { "type": "kernel", "key": "󰍛 ", "keyColor": "red" },
        {
            "type": "packages",
            "key": "󰏗 ",
            "keyColor": "green"
        },
        { "type": "memory", "key": " ", "keyColor": "magenta" },
        { "type": "shell", "key": " ", "keyColor": "blue" },
        { "type": "uptime", "key": "󱫐 ", "keyColor": "cyan" },
        {
            "type": "command",
            "key": "󱦟 ",
            "keyColor": "yellow",
            "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days"
        },
        "break",
        "colors"
    ]
}
